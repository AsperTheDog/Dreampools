{
	"loop2-father1": {
		"text": [
			{
				"text": "Ah, you're here!",
				"duration": 2.7,
				"character": "father"
			},
			{
				"text": "Welcome, Welcome!",
				"duration": 2.3,
				"character": "father"
			},
			{
				"text": "It's good to finally see you.",
				"duration": 2.6,
				"character": "father"
			},
			{
				"text": "For a moment I thought you would never appear!",
				"duration": 3.9,
				"character": "father"
			},
			{
				"text": "But don't worry, you are here now and that's what matters.",
				"duration": 7.0,
				"character": "father"
			},
			{
				"text": "I'm sure you will have a lovely time here.",
				"duration": 3.3,
				"character": "father"
			},
			{
				"text": "Take your time to enjoy every corner of the place,",
				"duration": 4.7,
				"character": "father"
			},
			{
				"text": "there is no need to rush!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 2 - line 1.wav"
	},
	"loop2-father2": {
		"text": [
			{
				"text": "Ahhh, that's good.",
				"duration": 3.2,
				"character": "father"
			},
			{
				"text": "Just the right temperature.",
				"duration": 3.7,
				"character": "father"
			},
			{
				"text": "I'm sure one could rest in these waters for hours and hours.",
				"duration": 5.9,
				"character": "father"
			},
			{
				"text": "So relaxing...",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 2 - line 2.wav"
	},
	"loop2-father3": {
		"text": [
			{
				"text": "Oh, are you getting out of the water? So fast?",
				"duration": 4.9,
				"character": "father"
			},
			{
				"text": "Was the water not to your liking?",
				"duration": 3.8,
				"character": "father"
			},
			{
				"text": "That's a shame...",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 2 - line 3.wav"
	},
	"loop2-father4": {
		"text": [
			{
				"text": "Are you leaving already?",
				"duration": 3.6,
				"character": "father"
			},
			{
				"text": "I'd hope you stay a bit longer here.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 2 - line 4.wav"
	},
	"loop3-father1": {
		"text": [
			{
				"text": "Oh, thank goodness, you are back.",
				"duration": 3.6,
				"character": "father"
			},
			{
				"text": "There's no need to rush, is there?",
				"duration": 5.7,
				"character": "father"
			},
			{
				"text": "You may miss some nice things in here if you run around like that, you know?",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 1.wav"
	},
	"loop3-father2": {
		"text": [
			{
				"text": "Oh! uh... That is not supposed to be there.",
				"duration": 5.7,
				"character": "father"
			},
			{
				"text": "I'm sorry for that, I don't know how it got there.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 2.wav"
	},
	"loop3-father3": {
		"text": [
			{
				"text": "Families are such a waste, aren't they?",
				"duration": 4.5,
				"character": "father"
			},
			{
				"text": "I'd rather stay here forever in the calm and quiet waters,\nwith no one to bother me at all...",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 3.wav"
	},
	"loop3-father4": {
		"text": [
			{
				"text": "Do you like it? It's a wonderful piece, I think.",
				"duration": 5.7,
				"character": "father"
			},
			{
				"text": "I wrote it myself and dare I say it,\nI am a master of my craft!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 4.wav"
	},
	"loop3-father5": {
		"text": [
			{
				"text": "Oh, the music stopped.",
				"duration": 3.2,
				"character": "father"
			},
			{
				"text": "I think you might have touched something by mistake.",
				"duration": 3.8,
				"character": "father"
			},
			{
				"text": "Could you put it back on?",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 5.wav"
	},
	"loop3-father6": {
		"text": [
			{
				"text": "What a wonderful view!",
				"duration": 3.3,
				"character": "father"
			},
			{
				"text": "I don't think you can enjoy a sunrise as beautiful\nas this one anywhere else, honestly.",
				"duration": 10.3,
				"character": "father"
			},
			{
				"text": "Makes me want to come sit here every day to see it.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 6.wav"
	},
	"loop3-father7": {
		"text": [
			{
				"text": "You aren't planning to leave again, are you?",
				"duration": 4.0,
				"character": "father"
			},
			{
				"text": "Everything is perfect here, why would you\neven consider leaving this place?",
				"duration": 5.6,
				"character": "father"
			},
			{
				"text": "Stay, please! Just a bit more!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 3 - line 7.wav"
	},
	"loop3-narrator1": {
		"text": [
			{
				"text": "Hello?",
				"duration": 3.5,
				"character": "narrator"
			},
			{
				"text": "Can anyone see this?",
				"duration": 3.0,
				"character": "narrator"
			},
			{
				"text": "I pray to god these subtitles work.",
				"duration": 4.5,
				"character": "narrator"
			},
			{
				"text": "To whoever, if anyone, is hearing this.\nI need your help, please.",
				"duration": 7.5,
				"character": "narrator"
			}
		]
	},
	"loop3-narrator2": {
		"text": [
			{
				"text": "It's all dark.",
				"duration": 2.5,
				"character": "narrator"
			},
			{
				"text": "I was supposed to go work as the narrator of\na game called Just Passing Through,",
				"duration": 7.5,
				"character": "narrator"
			},
			{
				"text": "but someone got in the way and trapped me in here.",
				"duration": 5.0,
				"character": "narrator"
			},
			{
				"text": "I don't know where I am...",
				"duration": 4.0,
				"character": "narrator"
			}
		]
	},
	"loop4-father1": {
		"text": [
			{
				"text": "Oh, you're back.",
				"duration": 2.9,
				"character": "father"
			},
			{
				"text": "Well, hopefully now you won't just leave again.",
				"duration": 4.2,
				"character": "father"
			},
			{
				"text": "I have to admit I am a bit hurt\nthat you left like that.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 4 - line 1.wav"
	},
	"loop4-father2": {
		"text": [
			{
				"text": "You have the music, you have the nice warm waters,\nyou have the views from the windows...",
				"duration": 8.3,
				"character": "father"
			},
			{
				"text": "I am at a loss, honestly.\nWhat more could you ask for?",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 4 - line 2.wav"
	},
	"loop4-father3": {
		"text": [
			{
				"text": "Is it me? Am I the problem?",
				"duration": 4.0,
				"character": "father"
			},
			{
				"text": "You don't enjoy my company?",
				"duration": 4.1,
				"character": "father"
			},
			{
				"text": "I am trying to do my best for you, you know?",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 4 - line 3.wav"
	},
	"loop4-father4": {
		"text": [
			{
				"text": "A letter? Don't mind those things,",
				"duration": 4.7,
				"character": "father"
			},
			{
				"text": "it probably didn't say anything important.",
				"duration": 4.8,
				"character": "father"
			},
			{
				"text": "Now please let's go back to the other room,\nI want to hear some more of that music.",
				"duration": 7.4,
				"character": "father"
			},
			{
				"text": "Do not leave again!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 4 - line 4.wav"
	},
	"loop5-father1": {
		"text": [
			{
				"text": "What... What's going on? Can you hear me??",
				"duration": 3.8,
				"character": "father"
			},
			{
				"text": "You shouldn't have left, now look at what happened.",
				"duration": 3.0,
				"character": "father"
			},
			{
				"text": "This is all your fault!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 5 - line 1.wav"
	},
	"loop5-father2": {
		"text": [
			{
				"text": "This is all your fault!\nYou are ruining everything!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 5 - line 2.wav"
	},
	"loop6-father1": {
		"text": [
			{
				"text": "You can still fix this, you know?",
				"duration": 2.9,
				"character": "father"
			},
			{
				"text": "You don't have to continue.",
				"duration": 2.8,
				"character": "father"
			},
			{
				"text": "I don't know what you are thinking but\nI implore you, just stop going forward.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 1.wav"
	},
	"loop6-father2": {
		"text": [
			{
				"text": "Stop, do not continue.\nThere is no going back if you continue.",
				"duration": 8.5,
				"character": "father"
			},
			{
				"text": "I know the previous place was way better\nbut here you still have some good things.",
				"duration": 7.2,
				"character": "father"
			},
			{
				"text": "Please, stop.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 2.wav"
	},
	"loop6-father3": {
		"text": [
			{
				"text": "Stop going forward!\nYou are going the wrong way!",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 3.wav"
	},
	"loop6-father4": {
		"text": [
			{
				"text": "They are not who they say they are,",
				"duration": 4.0,
				"character": "father"
			},
			{
				"text": "you never had a mother.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 4.wav"
	},
	"loop6-father5": {
		"text": [
			{
				"text": "I tried everything I could.\nDo I mean that little to you? ",
				"duration": 5.0,
				"character": "father"
			},
			{
				"text": "This is all because you refused to listen to me\nand continued being stubborn.",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 5.wav"
	},
	"loop6-father6": {
		"text": [
			{
				"text": "What have I done for you to hate me this much?",
				"duration": 4.5,
				"character": "father"
			},
			{
				"text": "Do you want to leave me that much?",
				"duration": 2.7,
				"character": "father"
			},
			{
				"text": "You enjoy making me suffer, right?",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 6.wav"
	},
	"loop6-father7": {
		"text": [
			{
				"text": "Please...",
				"duration": 3.7,
				"character": "father"
			},
			{
				"text": "Don't leave me again...",
				"duration": 0.0,
				"character": "father"
			}
		],
		"audio": "res://assets/audio/voiceover/Loop 6 - line 7.wav"
	}
}
